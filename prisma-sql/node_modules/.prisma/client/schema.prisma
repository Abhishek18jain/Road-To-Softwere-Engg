generator client {
  provider = "prisma-client-js"
}

model user {
  id   Int    @id @default(autoincrement())
  name String
  age  Int
}

model Author {
  id    Int    @id @default(autoincrement())
  name  String
  books Book[]
}

model Book {
  id       Int    @id @default(autoincrement())
  title    String
  AuthorId Int
  author   Author @relation(fields: [AuthorId], references: [id])
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
